{
  "name": "google-maps-vector-engine",
  "version": "0.2.0",
  "description": "High-performance TypeScript engine for rendering Mapbox Vector Tiles (MVT/PBF) on Google Maps with advanced features including fast feature lookups, interactive selections, dynamic styling, and optimized rendering.",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --project tsconfig.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:types": "tsc --project tsconfig.types.json",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:performance": "jest --config=jest.performance.config.js",
    "test:performance:report": "node scripts/run-performance-tests.js --output=performance-report.md",
    "test:performance:benchmark": "node scripts/run-performance-tests.js --benchmark --output=benchmark-report.md",
    "test:performance:memory": "node scripts/run-performance-tests.js --memory --output=memory-report.md",
    "test:performance:full": "node scripts/run-performance-tests.js --benchmark --memory --verbose --output=full-performance-report.md",
    "test:performance:integration": "jest tests/performance/MVTIntegration.performance.test.ts --config=jest.performance.config.js",
    "test:e2e": "jest tests/e2e",
    "test:visual": "jest tests/visual",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "eslint src/**/*.ts tests/**/*.ts --ext .ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"*.{js,json,md}\"",
    "check": "npm run lint && npm run format:check"
  },
  "keywords": [
    "google-maps",
    "vector-tiles",
    "mvt",
    "pbf",
    "typescript",
    "mapping",
    "gis",
    "performance",
    "interactive"
  ],
  "author": "Mohamed Ali Hachicha <medalihachicha@proton.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medali-07/google-maps-vector-engine.git"
  },
  "bugs": {
    "url": "https://github.com/medali-07/google-maps-vector-engine/issues"
  },
  "homepage": "https://github.com/medali-07/google-maps-vector-engine#readme",
  "peerDependencies": {
    "@types/google.maps": "^3.0.0"
  },
  "dependencies": {
    "@mapbox/vector-tile": "^2.0.4",
    "@turf/turf": "^7.2.0",
    "pbf": "^4.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/pbf": "3.0.5",
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@typescript-eslint/parser": "8.44.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "jest": "^29.0.0",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.0.0",
    "prettier": "3.6.2",
    "puppeteer": "^19.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "sideEffects": false,
  "funding": [
    {
      "type": "buy-me-a-coffee",
      "url": "https://buymeacoffee.com/medali.07"
    },
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/medalihachicha"
    }
  ],
  "publishConfig": {
    "access": "public"
  }
}
